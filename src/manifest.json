{
  "author": "Nano Core 2 contributors",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": {
      "128": "img/128_on.png"
    },
    "default_popup": "popup.html",
    "default_title": "Nano Adblocker",
    "browser_style": false
  },
  "commands": {
    "launch-element-picker": {
      "description": "__MSG_popupTipPicker__"
    },
    "launch-element-zapper": {
      "description": "__MSG_popupTipZapper__"
    },
    "launch-logger": {
      "description": "__MSG_popupTipLog__"
    }
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "js/vapi.js",
        "js/vapi-client.js",
        "js/vapi-usercss.js",
        "js/vapi-usercss.real.js",
        "js/contentscript.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*",
        "file://*/*"
      ],
      "run_at": "document_start"
    },
    {
      "all_frames": false,
      "js": [
        "js/scriptlets/subscriber.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_idle"
    }
  ],
  "default_locale": "en",
  "description": "An adblocker",
  "icons": {
    "128": "img/128_on.png"
  },
  "incognito": "spanning",
  "manifest_version": 2,
  "name": "Nano Adblocker",
  "options_ui": {
    "open_in_tab": true,
    "page": "dashboard.html"
  },
  "permissions": [
    "contextMenus",
    "privacy",
    "storage",
    "tabs",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "version": "1.0.0.90",
  "web_accessible_resources": [
    "web_accessible_resources/*"
  ],
  "sidebar_action": {
    "default_title": "__MSG_statsPageName__",
    "default_panel": "logger-ui.html",
    "default_icon": "img/128_on.png",
    "open_at_install": false
  },
  "applications": {
    "gecko": {
      "id": "{0f929014-5ed2-4527-8b8d-86a9c889b129}",
      "update_url": "https://raw.githubusercontent.com/LiCybora/NanoCoreFirefox/master/Extension%20Compiler/updates.json",
      "strict_min_version": "55.0"
    }
  }
}